package patmat

object patmatWkst {
  
  val testString = Huffman.string2Chars("Hello World")
                                                  //> testString  : List[Char] = List(H, e, l, l, o,  , W, o, r, l, d)
  val testTimes = Huffman.times(testString)       //> testTimes  : List[(Char, Int)] = List((d,1), (r,1), (W,1), (o,1), (l,1), (H,
                                                  //| 1), (e,1), (l,1), ( ,1), (o,1), (l,1))
}