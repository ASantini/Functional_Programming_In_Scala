package patmat


object patmatWkst {

  val testString = Huffman.string2Chars("H")      //> testString  : List[Char] = List(H)
  val testTimes = Huffman.times(testString)       //> testTimes  : List[(Char, Int)] = List((H,1))
  val testfreqs = Huffman.makeOrderedLeafList(testTimes)
                                                  //> testfreqs  : List[patmat.Huffman.Leaf] = List(Leaf(H,1))
}